# Google OAuth Credentials (REQUIRED - Same as Kodi plugin)
# You need to get these from Google Cloud Console
# Instructions: https://photos-kodi-addon.onrender.com/credentialsguide
GOOGLE_CLIENT_ID=your_client_id_here
GOOGLE_CLIENT_SECRET=your_client_secret_here

# Authentication Server (using the same server as the Kodi plugin)
AUTH_BASE_URL=https://photos-kodi-addon.onrender.com

# Flask Configuration
FLASK_ENV=development
SECRET_KEY=your_secret_key_here
